<p> ここは、<%=@user.name%>さんのページです。
</p>

<h3>all To do</h3>
<%  @cate_does.each do |cate_do| %>
  <tr>
  <td><%= cate_do[1] %></td>
  <br>
  </tr>
<% end %>

<%= form_with model: ToDoItem.new do |f| %>
 <%= f.label :contents, "To_do追加" %>
 <%= f.text_field :title %>
<%= f.select :category_id, @category_selects %>
 <%= f.submit %>
<% end %>


<h5>Categoryを追加する</h5>
<%= form_with model: Category.new do |f| %>
 <%= f.label :contents,  "category" %>
 <%= f.text_field :category_name %>
 <%= f.submit %>
<% end %>


<i class="material_icons">alarm</i>
<i class="material-icons md-36">alarm</i>
<i class="material-icons md-36">star_border</i>
<p> <%= link_to 'TO do', to_do_items_path %> </p>

